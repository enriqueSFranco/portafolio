---
export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width" />
  <link rel="icon" type="image/svg+xml" href="/logo.svg" />
  <meta
    name="description"
    content="curriculum vitae, cv, Frontend Developer, Desarrollo Web"
  />
  <meta name="author" content="Enrique SFranco" />
  <title>{title}</title>
  <script src="../scripts/setInitialTheme.ts"></script>
</head>
